
public class ClassA {
   public static void printNums() 
   {
	   
	   for(int i=0; i<6; i++) 
	   {
		 int  sum=1;
		   for(int j=0; j<=i; j++) 
		   {
			   System.out.print(sum+" ");
			   
			   sum++;
		   }
		   System.out.println();
	   }
   }
	public static void main(String[] args)
	
	{ 
		
		printNums();
		

	}

}
