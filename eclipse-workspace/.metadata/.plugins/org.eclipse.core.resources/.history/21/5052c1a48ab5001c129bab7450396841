package pkg2;

public class Exception_inJava {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
  public static void demo() 
  {
	 int a =100;
	 for(int i=0; i<10; i++) 
	 {
		 try {
			Thread.sleep(20000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
       System.out.println(a);
	 
	 }
  }
}
