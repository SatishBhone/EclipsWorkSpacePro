
public class ClassA {
   public static void printNums(int n) 
   {
	   int i,j, sum;
	   for(i=0; i<n; i++) 
	   {
		   sum=1;
		   for(j=0; j<=i; j++) 
		   {
			   System.out.print(sum+" ");
			   
			   sum++;
		   }
		   System.out.println();
	   }
   }
	public static void main(String[] args)
	
	{ 
		int n=6;
		printNums(n);
		

	}

}
