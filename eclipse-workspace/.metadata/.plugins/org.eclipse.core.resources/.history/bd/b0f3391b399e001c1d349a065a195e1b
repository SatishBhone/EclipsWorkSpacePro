
public class ClassA {
   public static void Method() 
   {
	   
	   for(int i=0; i<6; i++) 
	   {
		 int  a=1;
		   for(int j=0; j<=i; j++) 
		   {
			   System.out.print(a+" ");
			   
			   a++;
		   }
		   System.out.println();
	   }
   } 
	public static void main(String[] args)
	
	{ 
		
		Method();
		

	}

}
